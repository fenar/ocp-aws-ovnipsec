apiVersion: v1
baseDomain: narlabs.io
compute:
- architecture: amd64
  hyperthreading: Enabled
  name: worker
  replicas: 3
  platform:
    aws:
      rootVolume:
        iops: 2000
        size: 180
        type: io1
      type: m5.xlarge
      zones:
      - us-east-1a
      - us-east-1b
      - us-east-1c
controlPlane:
  architecture: amd64
  hyperthreading: Enabled
  name: master
  replicas: 3
  platform:
    aws:
      rootVolume:
        iops: 4000
        size: 140
        type: io1
      type: m5.xlarge
metadata:
  name: awsexpanded
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14
    hostPrefix: 23
  machineNetwork:
  - cidr: 10.0.0.0/16
  networkType: OVNKubernetes
  ovnKubernetesConfig:
    ipsecConfig: {}
  serviceNetwork:
  - 172.30.0.0/16
platform:
  aws:
    region: us-east-1
